<Window x:Class="POP31_MILOS_GUI.Prozori.NamestajEditProzor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP31_MILOS_GUI.Prozori"
        mc:Ignorable="d"
        Title="NamestajEditProzor" Height="350" Width="400" Background="Aquamarine">
    <Grid>
        <TextBox Name="tbNaziv" Text="{Binding Naziv}" HorizontalAlignment="Left" Height="23" Margin="249,52,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox Name="tbCena" Text="{Binding JedinicnaCena}" HorizontalAlignment="Left" Height="23" Margin="249,96,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox Name="tbKolicina" Text="{Binding KolicinaUMagacinu}" HorizontalAlignment="Left" Height="23" Margin="249,140,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox Name="tbSifra" Text="{Binding Sifra}" HorizontalAlignment="Left" Height="23" Margin="249,188,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <ComboBox Name="cbTip" SelectedItem="{Binding TipNamestaja}" DisplayMemberPath="Naziv" HorizontalAlignment="Left" Margin="249,232,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Name="btnOk" Content="Ok" HorizontalAlignment="Left" Margin="74,286,0,0" VerticalAlignment="Top" Width="75" Click="btnOk_Click"/>
        <Button Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="268,286,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>
        <Label Content="Naziv" HorizontalAlignment="Left" Margin="44,49,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.757,0.758"/>
        <Label Content="Cena" HorizontalAlignment="Left" Margin="44,96,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.198,0.539" Width="68"/>
        <Label Content="Kolicina" HorizontalAlignment="Left" Margin="44,137,0,0" VerticalAlignment="Top"/>
        <Label Content="Sifra" HorizontalAlignment="Left" Margin="44,184,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.146,0.414"/>
        <Label Content="Tip namestaja" HorizontalAlignment="Left" Margin="44,229,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.979,-0.813"/>

    </Grid>
</Window>
